                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">truncate table TBDWFT_ARTICLE_TO_NEMOSPED;

Select 
 a.article_cd as Codice_Articolo,
 case 
	when a.with_batch_flg =1 then 'SI'
	when a.with_batch_flg= 0 then 'NO' 
 end as Con_Lotto,
 case 
	when a.article_type_desc = 1 then 'Materie Prime'
	when a.article_type_desc = 2 then 'Semilavorato'
	when a.article_type_desc = 3 then 'Prod. Finiti'
	when a.article_type_desc = 5 then 'Mat. di confezionamento'
	when a.article_type_desc = 6 then 'Mat. di consumo'
	when a.article_type_desc = 8 then 'Servizi'
 end as Descrizione_Gruppo_Stock, 
 a.article_desc as Descrizione_Articolo,
 a.um_cd as Codice_UM,
 a.expiry_days_nbr as Scadenza_GG,
 cast (a.N_SKU_per_conf as int) as N_SKU_per_conf,
 case when a.N_SKU_per_conf = 0 then cast (a.N_BOX_per_pallet as int) else cast (a.N_BOX_per_pallet as int) * cast (a.N_SKU_per_conf as int) end  as N_SKU_per_pallet,
 case when len(a.GTIN_L1)=14 then substring(a.GTIN_L1,2,len(a.GTIN_L1))else a.GTIN_L1 end EAN13,
 a.GTIN_L1 as GTIN
from
(
	select 
		article_cd, with_batch_flg, article_desc, article_type_desc, um_cd, expiry_days_nbr,
		max(case when ADDED_VALUE_CD = 'Quantity L1 in L2|1013' then left(ADDED_VALUE_DESC,CHARINDEX('.', ADDED_VALUE_DESC)-1) end) as N_SKU_per_conf,
		max(case when ADDED_VALUE_CD = 'Contained Box in L3 (1003)' then  left(ADDED_VALUE_DESC,CHARINDEX('.', ADDED_VALUE_DESC)-1) end) as N_BOX_per_pallet,
		max(case when ADDED_VALUE_CD = 'GTIN L1' then ADDED_VALUE_DESC end) as GTIN_L1

	from
		(select id, article_cd, with_batch_flg, article_desc, article_type_desc, um_cd, expiry_days_nbr from [db_owner].TBDWAN_ARTICLE where site_id = ? and row_sts_flg = 1) a left join
		(select entity_id, entity_cd, added_value_cd, ADDED_VALUE_DESC from [db_owner].[TBDWFT_ADDED_VALUE] where site_id = ? and row_sts_flg = 1 and ENTITY_CATEGORY_CD='ARTICOLI' and ADDED_VALUE_CD in ('GTIN L1','Contained Box in L3 (1003)','Quantity L1 in L2|1013')) b on a.article_Cd = b.entity_cd
	group by article_cd, with_batch_flg, article_desc, article_type_desc, um_cd, expiry_days_nbr
) a</property>
